store.commit('ADD_MEMO' {item:temp, index:icon});\r\n        store.dispatch(\"fetchAddMemo\", { item: temp, index: icon });\r\n\r\n        resetItem();\r\n      } else {\r\n        showModal.value = true;\r\n      }\r\n    };\r\n\r\n    // 내용 재설정\r\n    const resetItem = () => {\r\n      newItem.value = \"\";\r\n    };\r\n\r\n    const addIcon = (index) => {\r\n      newIcon.value = index;\r\n    };\r\n\r\n    return {\r\n      newItem,\r\n      addItem,\r\n      addIcon,\r\n      showModal,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-wrap {\r\n  position: relative;\r\n  display: block;\r\n  line-height: 50px;\r\n  border-radius: 5px;\r\n  background-color: rgb(230, 251, 255);\r\n  overflow: hidden;\r\n  margin-top: 20px;\r\n}\r\n\r\n.input-wrap input {\r\n  border-style: none;\r\n}\r\n\r\n.input-wrap input:focus {\r\n  outline: none;\r\n}\r\n\r\n.input-box {\r\n  width: calc(100% - 200px);\r\n  font-size: 16px;\r\n  margin-left: 20px;\r\n  background-color: rgb(230, 251, 255);\r\n}\r\n\r\n.option {\r\n  position: absolute;\r\n  display: block;\r\n  right: 0;\r\n  top: 0;\r\n  width: 200px;\r\n}\r\n\r\n.img1:active,\r\n.img2:active,\r\n.img3:active {\r\n  outline: 3px solid rgb(65, 94, 138);\r\n  border-radius: 5px;\r\n}\r\n\r\n.img1 {\r\n  display: inline-block;\r\n  width: 50px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  font-size: 0;\r\n  cursor: pointer;\r\n  background: url(\"@/assets/images/dog1.png\") no-repeat center;\r\n  background-size: contain;\r\n}\r\n.img2 {\r\n  display: inline-block;\r\n  width: 50px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  font-size: 0;\r\n  cursor: pointer;\r\n  background: url(\"@/assets/images/dog2.png\") no-repeat center;\r\n  background-size: contain;\r\n}\r\n.img3 {\r\n  display: inline-block;\r\n  width: 50px;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  font-size: 0;\r\n  cursor: pointer;\r\n  background: url(\"@/assets/images/str.png\") no-repeat center;\r\n  background-size: contain;\r\n}\r\n\r\n.add-bt {\r\n  display: inline-block;\r\n  background-color: rgb(65, 94, 138);\r\n  cursor: pointer;\r\n}\r\n\r\n.add-bt-icon {\r\n  display: inline-block;\r\n  vertical-align: middle;\r\n  color: #fff;\r\n  margin: 0 20px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}