{"ast":null,"code":"import { ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport ModalView from \"@/components/common/ModalVue.vue\";\nexport default {\n  components: {\n    ModalView\n  },\n\n  setup() {\n    const store = useStore();\n    const newItem = ref(\"\");\n    const newIcon = ref(0);\n    const showModal = ref(false);\n\n    const addItem = () => {\n      let temp = newItem.value;\n      let icon = newIcon.value; // 앞쪽 뒷쪽 공백 제거\n\n      temp = temp.trim(); // 추후 업데이트 예정(정규표현식-문자열체크 문법)\n      //  앞자리공백   공백    뒷자리공백\n\n      if (temp !== \"\") {\n        // context.emit(\"additem\", temp, icon);\n        // store.commit('ADD_MEMO' {item:temp, index:icon});\n        store.dispatch(\"fetchAddMemo\", {\n          item: temp,\n          index: icon\n        });\n        resetItem();\n      } else {\n        showModal.value = true;\n      }\n    }; // 내용 재설정\n\n\n    const resetItem = () => {\n      newItem.value = \"\";\n    };\n\n    const addIcon = index => {\n      newIcon.value = index;\n    };\n\n    return {\n      newItem,\n      addItem,\n      addIcon,\n      showModal\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,SAASA,GAAT,QAAoB,KAApB;AAEA,SAASC,QAAT,QAAyB,MAAzB;AAEA,OAAOC,SAAP,MAAsB,kCAAtB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAKbE,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,EAAtB;IAEA,MAAMK,OAAM,GAAIN,GAAG,CAAC,EAAD,CAAnB;IACA,MAAMO,OAAM,GAAIP,GAAG,CAAC,CAAD,CAAnB;IACA,MAAMQ,SAAQ,GAAIR,GAAG,CAAC,KAAD,CAArB;;IAEA,MAAMS,OAAM,GAAI,MAAM;MACpB,IAAIC,IAAG,GAAIJ,OAAO,CAACK,KAAnB;MACA,IAAIC,IAAG,GAAIL,OAAO,CAACI,KAAnB,CAFoB,CAGpB;;MACAD,IAAG,GAAIA,IAAI,CAACG,IAAL,EAAP,CAJoB,CAKpB;MACA;;MACA,IAAIH,IAAG,KAAM,EAAb,EAAiB;QACf;QACA;QACAL,KAAK,CAACS,QAAN,CAAe,cAAf,EAA+B;UAAEC,IAAI,EAAEL,IAAR;UAAcM,KAAK,EAAEJ;QAArB,CAA/B;QAEAK,SAAS;MACX,CANA,MAMO;QACLT,SAAS,CAACG,KAAV,GAAkB,IAAlB;MACF;IACD,CAhBD,CAPM,CAyBN;;;IACA,MAAMM,SAAQ,GAAI,MAAM;MACtBX,OAAO,CAACK,KAAR,GAAgB,EAAhB;IACD,CAFD;;IAIA,MAAMO,OAAM,GAAKF,KAAD,IAAW;MACzBT,OAAO,CAACI,KAAR,GAAgB